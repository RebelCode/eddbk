{
    "name": "rebelcode/edd-bookings",
    "description": "The new EDD Bookings.",
    "type": "wordpress-plugin",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "RebelCode",
            "email": "dev@rebelcode.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": "^5.4 | ^7.0",
        "oomphinc/composer-installers-extender": "^1.0",
        "rebelcode/modular": "dev-task/initial-classes",
        "rebelcode/rcmod-wp-cqrs": "dev-task/initial-module-code",
        "rebelcode/rcmod-wp-bookings-cqrs": "dev-task/initial-module-code",
        "rebelcode/rcmod-eddbk-cqrs": "dev-task/initial-module-code",
        "rebelcode/rcmod-booking-logic": "dev-task/initial-module-code",
        "rebelcode/rcmod-eddbk-booking-logic": "dev-task/initial-module-code",
        "rebelcode/rcmod-eddbk-rest-api": "dev-task/initial-module-code",
        "rebelcode/rcmod-wp-bookings-shortcode": "dev-task/initial-implementation",
        "rebelcode/rcmod-wp-bookings-front-ui": "dev-task/initial-implementation",
        "rebelcode/rcmod-wp-bookings-ui": "dev-task/initial-implementation",
        "dhii/di": "^0.2-alpha1",
        "dhii/composite-container": "^0.1-alpha1",
        "dhii/memoize-memory": "^0.2-alpha1",
        "dhii/set": "^0.1-alpha1",
        "dhii/config": "^0.1-alpha1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "ptrofimov/xpmock": "^1.1",
        "dhii/php-cs-fixer-config": "^0.1",
        "codeclimate/php-test-reporter": "<=0.3.2"
    },
    "autoload": {
        "psr-4": {
            "RebelCode\\EddBookings\\Core\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit",
        "csfix": "php-cs-fixer fix -vvv"
    },
    "extra": {
        "installer-types": [
            "rc-module"
        ],
        "installer-paths": {
            "modules/{$name}/": [
                "type:rc-module"
            ]
        }
    }
}
